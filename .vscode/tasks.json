{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "args": ["/c"],
    "tasks": [
        {
            "label": "web-dotnet-build",
            "group": "build",
            "type": "process",
            "options": {
                "cwd": "${workspaceFolder}/ballast-web/dotnet"
            },
            "command": "dotnet",
            "args": [
                "build",
                "Ballast.Web.csproj"
            ],
            "problemMatcher": "$msCompile"
        }
    ]
}